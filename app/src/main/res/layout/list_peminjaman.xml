<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
                                                   xmlns:app="http://schemas.android.com/apk/res-auto"
                                                   xmlns:tools="http://schemas.android.com/tools"
                                                   android:layout_width="match_parent"
                                                   android:layout_height="wrap_content" android:padding="8dp"
                                                   android:layout_margin="8dp">

    <TextView
            android:text="TextView"
            android:layout_width="0dp"
            android:layout_height="wrap_content" android:id="@+id/txtTanggalPengembalianListPeminjaman"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/txtnamaListPeminjaman"
            android:layout_marginTop="8dp" app:layout_constraintHorizontal_bias="0.5"
            app:layout_constraintEnd_toStartOf="@+id/txtWaktuPengembalianListPengembalian" android:layout_marginEnd="4dp"/>
    <TextView
            android:text="TextView"
            android:layout_width="0dp"
            android:layout_height="wrap_content" android:id="@+id/txtWaktuPengembalianListPengembalian" app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="@+id/txtTanggalPengembalianListPeminjaman" app:layout_constraintStart_toEndOf="@+id/txtTanggalPengembalianListPeminjaman"
            app:layout_constraintHorizontal_bias="0.5" android:layout_marginStart="4dp"/>
    <TextView
            android:text="TextView"
            android:layout_width="0dp"
            android:layout_height="wrap_content" android:id="@+id/txtBukuListPeminjaman"
            app:layout_constraintTop_toTopOf="parent" app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent" android:textColor="#000000" android:textStyle="bold"
            android:gravity="center"/>
    <TextView
            android:text="TextView"
            android:layout_width="0dp"
            android:layout_height="wrap_content" android:id="@+id/txtnamaListPeminjaman"
            app:layout_constraintTop_toBottomOf="@+id/txtBukuListPeminjaman"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toStartOf="@+id/txtTanggalPeminjamanListPeminjaman"
            app:layout_constraintHorizontal_bias="0.5"
            android:layout_marginRight="4dp" android:layout_marginTop="8dp" android:textColor="#000000"/>
    <TextView
            android:text="TextView"
            android:layout_width="0dp"
            android:layout_height="wrap_content" android:id="@+id/txtTanggalPeminjamanListPeminjaman"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="@+id/txtnamaListPeminjaman"
            app:layout_constraintStart_toEndOf="@+id/txtnamaListPeminjaman"
            app:layout_constraintHorizontal_bias="0.5" android:layout_marginLeft="4dp" android:textColor="#000000"/>
</androidx.constraintlayout.widget.ConstraintLayout>
